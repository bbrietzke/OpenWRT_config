config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd1d:b917:3b0b::/48'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'wlan0'

config interface 'lan'
	option proto 'dhcp'
	option device 'eth0'

config interface 'wwan'
	option proto 'dhcp'